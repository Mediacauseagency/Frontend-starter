{
  "name": "Frontend-starter",
  "version": "1.0.0",
  "descrpition": "Frontend-starter",
  "main": "js/index.js",
  "scripts": {
    "watch-js": "watchify -v -t babelify js/index.js -o ../assets/build.js",
    "watch-css": "node-sass css/demo.scss ../assets/demo.css -w",
    "watch": "concurrently \"npm run watch-css\" \"npm run watch-js\"",
    "watch-demo-js": "watchify -v -t babelify js/demo.js -o ../assets/demo.js",
    "watch-demo": "open ../demo.html && concurrently \"npm run watch-css\" \"npm run watch-demo-js\"",
    "build-js": "browserify -t babelify -t uglifyify -p common-shakeify js/index.js | uglifyjs -c -m > ../assets/build.js && gzip -c -9 < ../assets/build.js > ../assets/build.js.gz",
    "build-demo-js": "browserify -t babelify -t uglifyify -p common-shakeify js/demo.js | uglifyjs -c -m > ../assets/demo.js",
    "build-demo-css": "postcss css/demo.css -c postcss.config.js --env 'production' -o ../assets/demo.css && gzip -c -9 < ../assets/demo.css > ../assets/demo.css.gz",
    "build-demo": "./build-demo.sh",
    "build-css": "postcss css/index.css -c postcss.config.js --env 'production' -o ../assets/build.css && gzip -c -9 < ../assets/build.css > ../assets/build.css.gz",
    "build": "npm install && npm run build-js && npm run build-css",
    "lint": "standard --fix | snazzy || true"
  },
  "author": "Yutaka Houlette <yutakahoulette@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "common-shakeify": "^0.4.5",
    "concurrently": "^3.5.0",
    "cssnano": "^3.10.0",
    "data-tooltip": "0.0.1",
    "node-sass": "^4.8.3",
    "normalize.css": "^8.0.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "tachyons": "^4.9.1",
    "tachyons-sass": "^4.9.2",
    "uglifyify": "^4.0.5",
    "uglifyjs": "^2.4.11",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "chart.js": "^2.7.1",
    "chartjs": "^0.3.24",
    "format-number": "^3.0.0"
  }
}
